#include <stdio.h>

int soma(int a, int b) {
    return a + b;
}

int main() {
    int n1, n2;

    printf("Digite o primeiro número: ");
    scanf("%d", &n1);
    printf("Digite o segundo número: ");
    scanf("%d", &n2);

    int resultado = soma(n1, n2);
    printf("SOMA = %d\n", resultado);

    return 0;
}
